    <dialogue-node nodeindex="0" editor-x="168" editor-y="70">
      <speak-char idTarget="Inigo">
        <resources name="No name" />
        <text> la puerta del sol, será mejor que cuides tus bolsillos,por aqui hay mucho pícaro </text>
      </speak-char>
      <condition />
      <speak-char idTarget="Inigo">
        <resources name="No name" />
